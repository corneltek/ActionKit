{
    "name": "corneltek/actionkit",
    "authors": [
        { "name": "Yo-An Lin", "email": "yoanlin93+composer@gmail.com" }
    ],
    "require": {
        "php": ">=5.6",
        "corneltek/cascading-attribute": "^1",
        "corneltek/fileutil": "^1.7",
        "universal/universal": "2.0.x-dev",
        "corneltek/imagekit": "^1",
        "corneltek/codegen": "@dev",
        "corneltek/cliframework": "@dev",
        "corneltek/formkit": "^1",
        "phifty/locale": "^3",
        "twig/twig": "^1|^2",
        "pekkis/mime-types": "^1",
        "pimple/pimple": "^3.0"
    },
    "require-dev": {
        "maghead/maghead": "4.0.x-dev",
        "maghead/magsql": "@dev",
        "corneltek/webserver-runner": "dev-master",
        "corneltek/kendo": "4.0.x-dev"
    },
    "autoload": {
        "psr-4": {
            "ActionKit\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests"],
        "psr-4": {
            "ProductBundle\\": "tests/ProductBundle/",
            "OrderBundle\\": "tests/OrderBundle/",
            "User\\": "tests/User/"
        }
    },
    "extra": { "branch-alias": { "dev-master": "4.0.x-dev" } },
    "license": "MIT"
}
